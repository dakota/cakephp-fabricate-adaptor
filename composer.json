{
    "name": "sizuhiko/cake_fabricate",
    "description": "CakeFabricate plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "3.0.x-dev",
        "sizuhiko/fabricate": "2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeFabricate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeFabricate\\Test\\": "tests",
            "CakeFabricate\\Test\\App\\": "tests/test_app/App",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
